@using MyVanity.Web.Helpers.MvcHelpers
@model List<MyVanity.Model.AgentModels.Impl.AgentEditModel>

@if (Model != null && Model.Count != 0)
{
    @Html.DropDownList("agents-dd", SelectListHelpers.BuilListOptions(Model, Model.First().Id, "Id", "FullName"), new { id = "agents-dropdown" })
}
else
{
    <h4>There are no other surgical coordinators to assign this patient</h4>
}

